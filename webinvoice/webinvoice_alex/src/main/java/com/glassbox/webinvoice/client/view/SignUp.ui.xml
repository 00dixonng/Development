<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
		font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<div class="wrapped-box">
			<table class="centered" cellpadding="10">
				<tr>
					<td>First Name *</td>
					<td>
						<g:TextBox ui:field="firstName" />
					</td>
				</tr>
				<tr>
					<td>Last Name *</td>
					<td>
						<g:TextBox ui:field="lastName" />
					</td>
				</tr>
				<tr>
					<td>Email Address *</td>
					<td>
						<g:TextBox ui:field="email" />
					</td>
				</tr>
				<tr>
					<td>Password *</td>
					<td>
						<g:PasswordTextBox ui:field="password" />
					</td>
				</tr>
				<tr>
					<td>Re-Password*</td>
					<td>
						<g:PasswordTextBox ui:field="password2" />
					</td>
				</tr>
				<tr>
					<td>Suburb *</td>
					<td>
						<g:TextBox ui:field="suburb" />
					</td>
				</tr>
				<tr>
					<td>Postcode *</td>
					<td>
						<g:TextBox ui:field="postcode" />
					</td>
				</tr>
				<tr>
					<td>State *</td>
					<td>
						<g:ListBox ui:field="state" />
					</td>
				</tr>
			</table>
			<g:Button text="Sign Up" ui:field="signUpBtn" styleName="btn btn-primary offset8"></g:Button>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 